    <PackageReference Include="System.Reflection.TypeExtensions" Version="4.4.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Rednet.Shared.Std\Rednet.Shared.Std.csproj" />
  </ItemGroup>

